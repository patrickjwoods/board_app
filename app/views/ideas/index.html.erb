<h1>Listing ideas</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Board</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ideas.each do |idea| %>
      <tr>
        <td><%= idea.board_id %></td>
        <td><%= idea.id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% # link_to 'New Idea', new_idea_path %>
